<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <LangVersion>7.3</LangVersion>
    <Version>1.3.5</Version>
    <RepositoryType>GIT</RepositoryType>
    <RepositoryUrl>https://github.com/petterek/Blitz.Rpc</RepositoryUrl>
    <PackageProjectUrl>https://github.com/petterek/Blitz.Rpc</PackageProjectUrl>
    <PackageTags>WebRpc dotnet</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Blitz.Rpc.Client\Blitz.Rpc.Client.csproj" />
    <ProjectReference Include="..\Blitz.Rpc.Shared\Blitz.Rpc.Shared.csproj" />
  </ItemGroup>

</Project>
